<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorblack</item>
        <item name="colorPrimaryDark">@color/colorblack</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:fontFamily">@font/robotolight</item>

    </style>
    <style name="AppTheme1" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorblack</item>
        <item name="colorPrimaryDark">@color/colorblack</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:fontFamily">@font/robotolight</item>

    </style>

    <style name="ColdStart" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/ll_win_bg_splash</item>
    </style>
    <style name="Theme_Dialog" parent="Base.Theme.AppCompat.Dialog">
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
    </style>


    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <!--<item name="android:fontFamily">@font/axiforma_regular</item>-->
    <style name="AppThemess" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorgreenlight</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:windowBackground">@color/colorblack</item>
    </style>


    <style name="AppThemes" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorblack</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="colorPrimaryDark">@color/colorblack</item>

        <item name="colorAccent">@color/colorblack</item>
        <item name="android:windowBackground">@color/colorblack</item>
    </style>



    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/colorblack</item>
        <item name="colorControlActivated">@color/gold_color</item>
    </style>

    <style name="SwitchTheme" parent="Theme.AppCompat.Light">
        <item name="android:colorControlActivated">#BAF814</item>
    </style>


    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_banner</item>
    </style>

    <style name="welcome_page">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingTop">100dp</item>
        <item name="android:paddingBottom">30dp</item>
        <item name="android:clipToPadding">false</item>
    </style>

    <style name="text_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="text_subtitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textColor">@color/lifgtwhi</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
    </style>

    <!-- Textview theme. -->
    <style name="bigtextviewnormalsmall" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorblack</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
